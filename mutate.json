{
  "adapters_tune": [
    {
      "adapter_name": "eth0",
      "rx": 4096,
      "tx": 4096
    },
    {
      "adapter_name": "eth1",
      "rx": 4096,
      "tx": 4096
    }
  ],
  "tuned_profile":
  {
    "profile": "mus",
    "path": "/usr/lib/tuned/mus/tuned.conf"
  },
  "environment": {
  "server": {
    "pod_spec": "pod_spec/iperf_server.yaml",
    "pod_name": "iperf-antrea-server-tester",
    "cmd": "iperf3",
    "port": 5201,
    "options": "--server -u --one-off --daemon",
    "role": "server"
  },
  "client": {
    "pod_spec": "pod_spec/iperf_client.yaml",
    "pod_name": "iperf-antrea-client-tester",
    "cmd": "iperf3",
    "port": 5201,
    "duration": 60,
    "parallel_streams": 4,
    "options": "-u --zerocopy -b 0",
    "role": "client"
  }
  }
}
